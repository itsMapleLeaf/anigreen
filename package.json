{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "run-p --race dev:*",
    "dev:remix": "remix dev",
    "dev:graphql": "pnpm run graphql --watch",
    "lint": "pnpm run graphql && run-s --continue-on-error lint:*",
    "lint:tsc": "tsc -b",
    "lint:eslint": "eslint --ext js,ts,tsx .",
    "lint:prettier": "prettier --check .",
    "format": "pnpm run graphql && run-s --continue-on-error format:*",
    "format:eslint": "eslint --ext js,ts,tsx . --fix",
    "format:prettier": "prettier --write .",
    "graphql": "graphql-codegen --config graphql.config.json"
  },
  "dependencies": {
    "@fontsource/fira-sans": "^4.5.10",
    "@fontsource/fira-sans-condensed": "^4.5.10",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@vercel/remix": "^1.15.0",
    "date-fns": "^2.29.3",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/typescript": "3.0.3",
    "@graphql-codegen/typescript-operations": "3.0.3",
    "@itsmapleleaf/configs": "^2.0.0",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "18"
  }
}
