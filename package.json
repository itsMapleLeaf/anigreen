{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "cross-env NODE_ENV=development run-p --race dev:*",
    "dev:server": "nodemon -w build dev-server.mjs",
    "dev:remix": "remix dev",
    "dev:graphql": "pnpm run graphql --watch",
    "dev:routes": "pnpm run routes --watch",
    "lint": "pnpm run graphql && pnpm run routes && run-s --continue-on-error lint:*",
    "lint:tsc": "tsc -b",
    "lint:eslint": "eslint --ext js,ts,tsx .",
    "lint:prettier": "prettier --check .",
    "format": "pnpm run graphql && pnpm run routes && run-s --continue-on-error format:*",
    "format:eslint": "eslint --ext js,ts,tsx . --fix",
    "format:prettier": "prettier --write .",
    "graphql": "graphql-codegen --config graphql.config.json",
    "routes": "routes-gen -d @routes-gen/remix"
  },
  "dependencies": {
    "@fontsource/fira-sans": "^4.5.10",
    "@fontsource/fira-sans-condensed": "^4.5.10",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@vercel/remix": "^1.15.0",
    "date-fns": "^2.29.3",
    "isbot": "^3.6.8",
    "lucide-react": "^0.161.0",
    "react": "^18.2.0",
    "react-aria-components": "1.0.0-alpha.3",
    "react-dom": "^18.2.0",
    "routes-gen": "^0.6.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/typescript": "3.0.3",
    "@graphql-codegen/typescript-operations": "3.0.3",
    "@itsmapleleaf/configs": "^2.0.0",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@remix-run/express": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@routes-gen/remix": "^0.3.6",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "18"
  }
}
